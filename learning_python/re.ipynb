    "import re\n",
    "#match从头整句匹配\n",
    "line = '正则表达式语法很easy,我爱正则表达式'\n",
    "regex = re.compile('(正则表达式语法很easy),(.*)')\n",
    "match_object = re.match(regex,line)\n",
    "print(match_object.group(1),match_object.group(2))"
